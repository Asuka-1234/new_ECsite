<div class="container">
	<div class="row row1 row2">
		<div class="col-xs-1"></div>
		<div class="col-xs-2 text search">
			<div>ジャンル検索</div>
			<div></div>
		</div>
		<div class="col-xs-1"></div>
		<div class="col-xs-8 clearfix">
			<div>
				<h3>商品一覧</h3>
			</div>
			<div>
				<% @items.each do |item| %>
				<div class="item user-item-detail">
					<div>
					    <%= attachment_image_tag item, :image, size: "130x90", fallback: "no_image.png" %>
					</div>
					<p><%= item.name %></p>
					<div>￥<%= item.price %></div>
				</div>
				<% end %>
			</div>
		</div>
	</div>
</div>